{
  "manifest_version": 2,

  "name": "Social Lives Matter",
  "description": "Content script to find and replace tweets",
  "version": "1.0",

  "browser_action": {
    "default_icon": "bee.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["myScript.js"]
    }
  ]
}
